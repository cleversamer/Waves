{
  "server": {
    "url": "http://192.168.1.235:4000",
    "routes": {
      "getAllProducts": "/api/products"
    }
  },
  "routes": {
    "home": "/",
    "notFound": "/notFound",
    "shop": "/shop",
    "login": "/login",
    "dashboard": "/dashboard",
    "cart": "/dashboard/user/cart",
    "productDetails": "/product"
  },
  "query": {
    "products": {
      "bySold": {
        "skip": 0,
        "limit": 4,
        "sortBy": "itemSold",
        "order": -1
      },
      "byDate": {
        "skip": 0,
        "limit": 4,
        "sortBy": "date",
        "order": -1
      }
    }
  },
  "paths": {
    "notAvailableImage": "/assets/images/image_not_available.png"
  },
  "errors": {
    "default": "Something went wrong.",
    "fetch": {
      "products": "Could not fetch products."
    }
  }
}
